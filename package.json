{
  "name": "shred_custody",
  "version": "1.0.0",
  "description": "Fireblocks TypeScript SDK examples demonstrating custody operations, vault management, and transaction workflows",
  "main": "dist/client.js",
  "scripts": {
    "example:list-default": "ts-node src/examples/example-list-default-accounts.ts",
    "example:list-assets": "ts-node src/examples/example-list-accounts-with-assets.ts",
    "example:transfer-vault": "ts-node src/examples/example-transfer-to-vault.ts",
    "example:transfer-address": "ts-node src/examples/example-transfer-to-external-address.ts",
    "example:transfer-wallet": "ts-node src/examples/example-transfer-to-external-wallet.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "dependencies": {
    "@fireblocks/ts-sdk": "^12.1.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
